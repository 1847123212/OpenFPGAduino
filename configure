HOST=arm-none-linux-gnueabi
cd node; \
AR="${HOST}-ar" \
STRIP="${HOST}-strip" \
OBJCOPY="${HOST}-objcopy" \ 
LD="${HOST}-ld" \
OBJDUMP="${HOST}-objdump" \
CC="${HOST}-gcc" \
CXX="${HOST}-g++" \
NM="${HOST}-nm" \
AS="${HOST}-as" \
./configure --prefix=PREFIX --dest-cpu=arm --dest-os=linux --with-arm-float-abi=soft; cd ..
